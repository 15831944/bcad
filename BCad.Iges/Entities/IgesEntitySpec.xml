<?xml version="1.0" encoding="utf-8" ?>
<Spec xmlns="http://IxMilia.com/Iges/EntitiesSpec">
  <!--
  
  100 - Circular arc
  
  -->
  <Entity Name="IgesCircularArc" EntityType="CircularArc" TypeCode="100">
    <Property Name="PlaneDisplacement" Type="double" DefaultValue="0.0" />
    <Property Name="Center" Type="IgesPoint" DefaultValue="IgesPoint.Origin"/>
    <Property Name="StartPoint" Type="IgesPoint" DefaultValue="IgesPoint.Origin"/>
    <Property Name="EndPoint" Type="IgesPoint" DefaultValue="IgesPoint.Origin"/>
    <ParameterOrder>
      <Parameter Value="PlaneDisplacement" Converter="Double" />
      <Parameter Value="Center.X" Converter="Double" />
      <Parameter Value="Center.Y" Converter="Double" />
      <Parameter Value="StartPoint.X" Converter="Double" />
      <Parameter Value="StartPoint.Y" Converter="Double" />
      <Parameter Value="EndPoint.X" Converter="Double" />
      <Parameter Value="EndPoint.Y" Converter="Double" />
    </ParameterOrder>
  </Entity>
  <!--
  
  110 - Line
  
  -->
  <Entity Name="IgesLine" EntityType="Line" TypeCode="110">
    <Property Name="P1" Type="IgesPoint" DefaultValue="IgesPoint.Origin" />
    <Property Name="P2" Type="IgesPoint" DefaultValue="IgesPoint.Origin" />
    <ParameterOrder>
      <Parameter Value="P1.X" Converter="Double" />
      <Parameter Value="P1.Y" Converter="Double" />
      <Parameter Value="P1.Z" Converter="Double" />
      <Parameter Value="P2.X" Converter="Double" />
      <Parameter Value="P2.Y" Converter="Double" />
      <Parameter Value="P2.Z" Converter="Double" />
    </ParameterOrder>
    <CustomProperty Name="LineCount" Type="int" Overrides="true">
      <Get><![CDATA[return 1;]]></Get>
    </CustomProperty>
    <CustomProperty Name="Bounding" Type="IgesBounding">
      <Get><![CDATA[return (IgesBounding)FormNumber;]]></Get>
      <Set><![CDATA[FormNumber = (int)value;]]></Set>
    </CustomProperty>
  </Entity>
  <!--
  
  116 - IgesLocation
  
  -->
  <Entity Name="IgesLocation" EntityType="Point" TypeCode="116">
    <Property Name="Location" Type="IgesPoint" DefaultValue="IgesPoint.Origin" />
    <ParameterOrder>
      <Parameter Value="Location.X" Converter="Double" />
      <Parameter Value="Location.Y" Converter="Double" />
      <Parameter Value="Location.Z" Converter="Double" />
    </ParameterOrder>
  </Entity>
  <!--
  
  124 - Transformation matrix
  
  -->
  <Entity Name="IgesTransformationMatrix" EntityType="TransformationMatrix" TypeCode="124">
    <Property Name="R11" Type="double" DefaultValue="0.0" />
    <Property Name="R12" Type="double" DefaultValue="0.0" />
    <Property Name="R13" Type="double" DefaultValue="0.0" />
    <Property Name="T1" Type="double" DefaultValue="0.0" />
    <Property Name="R21" Type="double" DefaultValue="0.0" />
    <Property Name="R22" Type="double" DefaultValue="0.0" />
    <Property Name="R23" Type="double" DefaultValue="0.0" />
    <Property Name="T2" Type="double" DefaultValue="0.0" />
    <Property Name="R31" Type="double" DefaultValue="0.0" />
    <Property Name="R32" Type="double" DefaultValue="0.0" />
    <Property Name="R33" Type="double" DefaultValue="0.0" />
    <Property Name="T3" Type="double" DefaultValue="0.0" />
    <ParameterOrder>
      <Parameter Value="R11" Converter="Double" />
      <Parameter Value="R12" Converter="Double" />
      <Parameter Value="R13" Converter="Double" />
      <Parameter Value="T1" Converter="Double" />
      <Parameter Value="R21" Converter="Double" />
      <Parameter Value="R22" Converter="Double" />
      <Parameter Value="R23" Converter="Double" />
      <Parameter Value="T2" Converter="Double" />
      <Parameter Value="R31" Converter="Double" />
      <Parameter Value="R32" Converter="Double" />
      <Parameter Value="R33" Converter="Double" />
      <Parameter Value="T3" Converter="Double" />
    </ParameterOrder>
  </Entity>
  <!--

  308 - Subfigure definition
  
  -->
  <Entity Name="IgesSubfigureDefinition" EntityType="SubfigureDefinition" TypeCode="308">
    <Property Name="Depth" Type="int" DefaultValue="0" />
    <Property Name="Name" Type="string" DefaultValue="null" />
    <Property Name="EntityCount" Type="int" DefaultValue="0" Accessibility="private" />
    <CustomProperty Name="Entities" Type="List&lt;IgesEntity&gt;">
      <Get><![CDATA[return SubEntities;]]></Get>
    </CustomProperty>
    <ParameterOrder>
      <Parameter Value="Depth" Converter="Integer" />
      <Parameter Value="Name" Converter="String" />
      <Parameter Value="EntityCount" Converter="Integer" Mode="ReadOnly" />
      <Parameter Value="Entities.Count" Converter="Integer" Mode="WriteOnly" />
      <Parameter Value="SubEntityIndices" Converter="Integer" Count="EntityCount" />
    </ParameterOrder>
  </Entity>
</Spec>
