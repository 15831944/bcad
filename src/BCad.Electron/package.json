{
  "name": "bcad",
  "version": "1.0.0",
  "description": "A minimal Electron application written with Typescript",
  "main": "./dist/main.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "pack": "build-server.cmd --configuration Release && electron-packager . --platform=win32 --arch=x64 --out=pack --overwrite",
    "start": "npm run build && electron ./dist/main.js -- debug local"
  },
  "repository": "https://github.com/electron/electron-quick-start-typescript",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo",
    "typescript"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^6.0.8",
    "electron-packager": "^14.0.6",
    "tslint": "^5.19.0",
    "typescript": "^3.6.0"
  },
  "dependencies": {
    "resize-observer": "1.0.0",
    "vscode-jsonrpc": "^4.0.0"
  }
}
